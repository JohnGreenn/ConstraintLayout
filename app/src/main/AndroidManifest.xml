<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xq.constraintlayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.xq.constraintlayout.Sample">

        </activity>
        <activity android:name="com.xq.constraintlayout.CircularPositioning" />
        <activity android:name="com.xq.constraintlayout.Helpers" />
        <activity android:name="com.xq.constraintlayout.CircularReveal" />
        <activity android:name="com.xq.constraintlayout.PlaceHolder" />
        <activity android:name="com.xq.constraintlayout.ConstraintSetX" />
        <activity android:name="com.xq.constraintlayout.LinearActivity" />
        <activity android:name="com.xq.constraintlayout.FlowActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>